<div class="repo-contain">
  <div class= "repo-info">
    <h2 class="repo-title">{{name}}</h2>
    <h3 class="repo-desc">{{description}}</h3>
    <h4 class="repo-update">Updated {{updatedDate}}</h4>
  </div>
  <div class = "file-icons">
    <ul> 
      <li class="file-type">{{language}}</li>
      <li class="octicon-icon"><span class="octicon octicon-star">{{stargazers_count}}</span></li>
      <li class="octicon-icon"><span class="octicon octicon-git-branch">{{forks_count}}</span></li>
    </ul>
  </div>
</div>