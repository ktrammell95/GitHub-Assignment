<div class = "file-icons">
  <ul>
    <li class="file-type">{{language}}</li>
    <li class="octicon-icon"><span class="octicon octicon-star">0</span></li>
    <li class="octicon-icon"><span class="octicon octicon-git-branch">0</span></li>
  </ul>
</div>
<div class="repo-info">
  <h2 class="repo-title">{{name}}</h2>
  <h3 class="repo-desc">{{description}}</h3>
  <h4 class="repo-update">Updated 2 days ago</h4>
</div>